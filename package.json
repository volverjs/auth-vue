{
    "name": "@volverjs/auth-vue",
    "description": "Simple OAuth 2 / OpenID Connect plugin for Vue 3",
    "author": "8 Wave S.r.l.",
    "license": "MIT",
    "keywords": [
        "oauth",
        "openid",
        "vue3",
        "storage"
    ],
    "homepage": "https://github.com/volverjs/auth-vue",
    "repository": {
        "type": "git",
        "url": "https://github.com/volverjs/auth-vue"
    },
    "bugs": {
        "url": "https://github.com/volverjs/auth-vue/issues"
    },
    "version": "0.0.0",
    "engines": {
        "node": ">= 16.x"
    },
    "packageManager": "pnpm@9.1.4",
    "scripts": {
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "type-check": "tsc --noEmit",
        "dev": "vite build --watch",
        "build": "vite build",
        "test": "npm run test-vitest",
        "test-vitest": "vitest run",
        "test-vitest-watch": "vitest",
        "credits": "npx @opengovsg/credits-generator"
    },
    "type": "module",
    "sideEffects": false,
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "src",
        "*.d.ts"
    ],
    "dependencies": {
        "oauth4webapi": "^2.10.4",
        "vue": "^3.4.27"
    },
    "devDependencies": {
        "@nabla/vite-plugin-eslint": "^2.0.4",
        "@typescript-eslint/eslint-plugin": "^7.12.0",
        "@typescript-eslint/parser": "^7.12.0",
        "copy": "^0.3.2",
        "eslint": "^9.4.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "happy-dom": "^14.12.0",
        "node-fetch": "^3.3.2",
        "prettier": "^3.3.1",
        "typescript": "^5.4.5",
        "vite": "^5.2.12",
        "vite-plugin-dts": "^3.9.1",
        "vitest": "^1.6.0",
        "vitest-fetch-mock": "^0.2.2",
        "vue-tsc": "^2.0.19"
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/index.d.ts"
            ],
            "storage": [
                "dist/Storage.d.ts"
            ],
            "local-storage": [
                "dist/LocalStorage.d.ts"
            ],
            "session-storage": [
                "dist/SessionStorage.d.ts"
            ],
            "oauth-client": [
                "dist/OauthClient.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./src/*": "./src/*",
        "./dist/*": "./dist/*",
        "./storage": {
            "types": "./dist/Storage.d.ts",
            "import": "./dist/Storage.js"
        },
        "./local-storage": {
            "types": "./dist/LocalStorage.d.ts",
            "import": "./dist/LocalStorage.js"
        },
        "./session-storage": {
            "types": "./dist/SessionStorage.d.ts",
            "import": "./dist/SessionStorage.js"
        },
        "./oauth-client": {
            "types": "./dist/OauthClient.d.ts",
            "import": "./dist/OauthClient.js"
        }
    }
}